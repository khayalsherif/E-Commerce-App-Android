<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Ecommerceappandroid" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">true</item>
        <item name="android:background">@color/light_screen_background_color</item>
        <!-- Customize your theme here. -->
        <item name="hint_color">@color/light_hint_color</item>
        <item name="screen_title_color">@color/light_screen_title_color</item>
        <item name="text_color">@color/light_text_color</item>
        <item name="back_icon_color">@color/light_back_icon_color</item>
        <item name="button_is_enable_color">@color/light_button_is_enable_color</item>
        <item name="button_is_not_enable_color">@color/light_button_is_not_enable_color</item>
        <item name="line_color">@color/light_line_color</item>
        <item name="edit_text_text_color">@color/light_edit_text_text_color</item>
        <item name="screen_background_color">@color/light_screen_background_color</item>
        <item name="button_is_enable_text_color">@color/light_button_is_enable_text_color</item>
        <item name="button_is_not_enable_text_color">@color/light_button_is_not_enable_text_color</item>
        <item name="edit_text_stroke_selected_color">@color/light_edit_text_stroke_selected_color</item>
        <item name="edit_text_stroke_un_selected_color">@color/light_edit_text_stroke_un_selected_color</item>
        <item name="edit_text_background_color">@color/light_edit_text_background_color</item>
        <item name="carrot_accent">@color/carrot_accent</item>
    </style>
</resources>